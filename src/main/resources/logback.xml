<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{yyyy-MM-dd HH:mm:ss} [%t] %p: %m%n</pattern>
        </encoder>
    </appender>

    <logger name="org.jooq.impl.AbstractQueryPart$SerializationDeprecation" level="OFF"/>
    <root level="info">
        <appender-ref ref="STDOUT" />
    </root>
</configuration>
